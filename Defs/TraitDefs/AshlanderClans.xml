<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- Ahemmusa Clan - Peaceful, wise, enduring -->
  <TraitDef>
    <defName>AhemmusaClan</defName>
    <label>Ahemmusa Clan</label>
    <description>Member of the peaceful Ahemmusa clan, known for wisdom, endurance, and deep spiritual understanding of the ashlands.</description>
    <degreeDatas>
      <li>
        <label>Ahemmusa Clanmate</label>
        <description>A member of the wise and peaceful Ahemmusa clan, raised to value knowledge and endurance over violence.</description>
        <skillGains>
          <Medicine>3</Medicine>
          <Intellectual>2</Intellectual>
          <Social>2</Social>
        </skillGains>
        <statOffsets>
          <GlobalLearningFactor>0.15</GlobalLearningFactor>
          <PainShockThreshold>0.10</PainShockThreshold>
          <ImmunityGainSpeed>1.2</ImmunityGainSpeed>
        </statOffsets>
      </li>
      <li>
        <label>Ahemmusa Elder</label>
        <description>A wise elder of the Ahemmusa, deeply versed in clan lore and the healing arts of the ashlands.</description>
        <skillGains>
          <Medicine>4</Medicine>
          <Intellectual>3</Intellectual>
          <Social>3</Social>
          <Artistic>1</Artistic>
        </skillGains>
        <statOffsets>
          <GlobalLearningFactor>0.25</GlobalLearningFactor>
          <PainShockThreshold>0.15</PainShockThreshold>
          <ImmunityGainSpeed>1.4</ImmunityGainSpeed>
          <MedicalTendQuality>0.20</MedicalTendQuality>
        </statOffsets>
      </li>
    </degreeDatas>
  </TraitDef>

