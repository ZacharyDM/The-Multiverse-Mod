      </li>
    </stockGenerators>
  </TraderKindDef>

  <!-- Orsimer Special Resources Trader -->
  <TraderKindDef>
    <defName>OrsimerSpecialResourceTrader</defName>
    <label>Orsimer resource trader</label>
    <stockGenerators>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Malachite</thingDef>
        <totalPriceRange>350~850</totalPriceRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Ebony</thingDef>
