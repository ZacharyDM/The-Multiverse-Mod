<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!-- Dunmer Special Resources Trader -->
  <TraderKindDef>
    <defName>DunmerSpecialResourceTrader</defName>
    <label>Dunmer resource trader</label>
    <stockGenerators>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Malachite</thingDef>
        <totalPriceRange>300~800</totalPriceRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Ebony</thingDef>
        <totalPriceRange>600~1500</totalPriceRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>ResourcesRaw</categoryDef>
        <thingDefCountRange>2~6</thingDefCountRange>
        <totalPriceRange>200~500</totalPriceRange>
      </li>
    </stockGenerators>
  </TraderKindDef>

  <!-- Dwemer Special Resources Trader -->
