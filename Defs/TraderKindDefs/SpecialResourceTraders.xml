  <!-- Orsimer Animal Trader -->
  <TraderKindDef>
    <defName>OrsimerAnimalTrader</defName>
    <label>Orsimer beast trader</label>
    <stockGenerators>
      <li Class="StockGenerator_Animals">
        <tradeTagsSell>
          <li>AnimalOrsimerPack</li>
          <li>AnimalOrsimerWar</li>
          <li>AnimalOrsimerLivestock</li>
        </tradeTagsSell>
        <tradeTagsBuy>
          <li>AnimalOrsimerPack</li>
          <li>AnimalOrsimerWar</li>
          <li>AnimalOrsimerLivestock</li>
        </tradeTagsBuy>
        <kindCountRange>2~5</kindCountRange>
        <countRange>1~3</countRange>
        <totalPriceRange>600~2000</totalPriceRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>AnimalProductRaw</categoryDef>
        <thingDefCountRange>1~4</thingDefCountRange>
        <totalPriceRange>100~300</totalPriceRange>
      </li>
    </stockGenerators>
  </TraderKindDef>

</Defs>