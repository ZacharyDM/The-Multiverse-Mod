        <thingDef>Malachite</thingDef>
        <totalPriceRange>400~900</totalPriceRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Ebony</thingDef>
        <totalPriceRange>800~2000</totalPriceRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>DwarvenMetal</thingDef>
        <totalPriceRange>500~1200</totalPriceRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>ResourcesRaw</categoryDef>
        <thingDefCountRange>3~8</thingDefCountRange>
