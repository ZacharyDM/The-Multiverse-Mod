    <defName>TribunalDeityNames</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[TribunalGodName]</li>
        
        <li>TribunalGodName->Almalexia</li>
        <li>TribunalGodName->Sotha Sil</li>
        <li>TribunalGodName->Vivec</li>
        <li>TribunalGodName->The Healing Mother</li>
        <li>TribunalGodName->The Clockwork God</li>
        <li>TribunalGodName->The Poet-Warrior</li>
        <li>TribunalGodName->Indoril Nerevar</li>
        <li>TribunalGodName->Saint Veloth</li>
        <li>TribunalGodName->House Founder</li>
        <li>TribunalGodName->Noble Ancestor</li>
        <li>TribunalGodName->House Patriarch</li>
        <li>TribunalGodName->House Matriarch</li>
        <li>TribunalGodName->First Councilor</li>
        <li>TribunalGodName->Elder of the House</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <!-- House Dagoth Deity Names -->
