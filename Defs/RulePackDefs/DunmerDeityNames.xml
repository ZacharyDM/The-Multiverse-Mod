    <defName>DagothDeityNames</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[DagothGodName]</li>
        
        <li>DagothGodName->Dagoth Ur</li>
        <li>DagothGodName->The Sharmat</li>
        <li>DagothGodName->The Dreamer</li>
        <li>DagothGodName->Lord of the Sixth House</li>
        <li>DagothGodName->The Dwemer's Bane</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

