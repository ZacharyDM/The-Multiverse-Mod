    <defName>DaedraDeityNames</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[DaedraGodName]</li>
        
        <li>DaedraGodName->Azura</li>
        <li>DaedraGodName->Boethiah</li>
        <li>DaedraGodName->Mephala</li>
        <li>DaedraGodName->Dusk and Dawn</li>
        <li>DaedraGodName->Prince of Plots</li>
        <li>DaedraGodName->The Webspinner</li>
        <li>DaedraGodName->Anticipation of Sotha Sil</li>
        <li>DaedraGodName->Anticipation of Almalexia</li>
        <li>DaedraGodName->Anticipation of Vivec</li>
