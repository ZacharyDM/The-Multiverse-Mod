<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!-- Kagesh Clan Ritual Robes -->
  <ThingDef ParentName="ApparelMakeableBase">
    <defName>Apparel_KageshRitualRobes</defName>
    <label>Kagesh ritual robes</label>
    <description>Mysterious dark robes worn by members of the Kagesh clan. These garments are covered in arcane symbols and ritual markings that speak to the clan's secretive traditions and connection to hidden knowledge.</description>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/Ashlanders/Kagesh/Kagesh</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costStuffCount>65</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
    </stuffCategories>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <statBases>
      <WorkToMake>4800</WorkToMake>
      <MaxHitPoints>95</MaxHitPoints>
      <Mass>1.4</Mass>
      <Flammability>0.9</Flammability>
      <ArmorRating_Sharp>0.08</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.04</ArmorRating_Blunt>
      <ArmorRating_Heat>0.30</ArmorRating_Heat>
      <Insulation_Cold>16</Insulation_Cold>
      <Insulation_Heat>9</Insulation_Heat>
      <EquipDelay>3</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <ResearchSpeed>0.08</ResearchSpeed>
      <PsychicSensitivity>0.15</PsychicSensitivity>
      <MentalBreakThreshold>-0.03</MentalBreakThreshold>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Shoulders</li>
        <li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Pawn/Humanlike/Apparel/Ashlanders/Kagesh/Kagesh</wornGraphicPath>
      <layers>
        <li>Middle</li>
      </layers>
      <tags>
        <li>AshlanderClothing</li>
        <li>KageshClan</li>
      </tags>
      <defaultOutfitTags>
        <li>Worker</li>
      </defaultOutfitTags>
    </apparel>
    <recipeMaker>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
      <skillRequirements>
        <Crafting>4</Crafting>
      </skillRequirements>
      <recipeUsers>
        <li>ElectricTailoringBench</li>
        <li>HandTailoringBench</li>
      </recipeUsers>
    </recipeMaker>
  </ThingDef>

