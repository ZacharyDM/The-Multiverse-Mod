<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!-- Bull Netch: Large floating gas-filled creature -->
  <ThingDef ParentName="AnimalThingBase">
    <defName>BullNetch</defName>
    <label>bull netch</label>
    <description>A massive, floating creature that drifts through Morrowind's skies using internal gas bladders. Bull netch are the larger, more aggressive males of the species, capable of powerful electrical attacks and territorial behavior.</description>
    <statBases>
      <MoveSpeed>4.5</MoveSpeed>
      <MarketValue>800</MarketValue>
      <ComfyTemperatureMin>0</ComfyTemperatureMin>
      <ComfyTemperatureMax>55</ComfyTemperatureMax>
      <FilthRate>12</FilthRate>
    </statBases>
    <tools>
      <li>
        <label>shock tentacles</label>
        <capacities>
          <li>Electric</li>
        </capacities>
        <power>18</power>
        <cooldownTime>3.0</cooldownTime>
        <linkedBodyPartsGroup>TentacleAttackTool</linkedBodyPartsGroup>
        <chanceFactor>0.6</chanceFactor>
      </li>
      <li>
        <label>ram</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>22</power>
        <cooldownTime>3.5</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
      </li>
    </tools>
    <race>
      <body>BirdLike</body>
      <predator>false</predator>
      <petness>0.05</petness>
      <baseBodySize>2.5</baseBodySize>
      <baseHungerRate>0.6</baseHungerRate>
      <baseHealthScale>2.0</baseHealthScale>
      <foodType>VegetarianRoughAnimal</foodType>
      <leatherDef>Leather_Heavy</leatherDef>
      <wildness>0.95</wildness>
      <manhunterOnTameFailChance>0.3</manhunterOnTameFailChance>
      <manhunterOnDamageChance>0.7</manhunterOnDamageChance>
      <canFly>true</canFly>
      <lifeExpectancy>25</lifeExpectancy>
      <mateMtbHours>8760</mateMtbHours>
      <maturityAge>4.0</maturityAge>
      <trainability>None</trainability>
      <gestationPeriodDays>120</gestationPeriodDays>
      <litterSizeCurve>
        <points>
          <li>(0.5, 0)</li>
          <li>(1, 1)</li>
          <li>(2, 0)</li>
        </points>
      </litterSizeCurve>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>1.0</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>4.0</minAge>
          <soundWounded>Pawn_Thrumbo_Wounded</soundWounded>
          <soundDeath>Pawn_Thrumbo_Death</soundDeath>
          <soundCall>Pawn_Thrumbo_Call</soundCall>
          <soundAngry>Pawn_Thrumbo_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
    </race>
  </ThingDef>

